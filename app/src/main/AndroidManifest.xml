<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidstudy">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.baselibrary.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidStudy"
        android:usesCleartextTraffic="true">
        <activity android:name=".any.db.SqlLiteDBActivity"></activity>
        <activity android:name=".hencoder.HenCoderTestActivity" />
        <activity android:name=".hencoder.HenCoderActivity" />
        <activity android:name=".any.customview.CustomViewActivity" />
        <activity android:name=".any.nestedscroll.NestedScrollActivity" />
        <activity android:name=".any.coordinatorlayout.CoordinatorLayoutTestActivity" />
        <activity android:name=".any.addressphoto.activity.PhotoActivity" />
        <activity android:name=".any.fonttest.FontTestActivity" />
        <activity android:name=".any.addressphoto.activity.AddressPhotoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>